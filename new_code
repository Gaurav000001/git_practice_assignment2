function checkPrime(num){
    var count=0;
    for(let i=1;i<=num;i++){
        if(num%i == 0){
            count++;
        }
    }

    if(count == 2){
        return true;
    }else{
        return false;
    }
}

if(checkPrime(13)){
    console.log("Number is prime");
}
else{
    console.log("Number is Not prime");
}